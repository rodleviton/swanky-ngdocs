<h2><%- doc.name -%></h1>

<div class="<%-styles['api-section']-%>">
  <%- doc.description -%>
</div>

<% if (doc.knownIssues) { %>
<h3>Known Issues</h3>
<% doc.knownIssues.forEach((issue) => { %>
<div class="<%-styles['known-issue']-%>">
  <%- issue -%>
</div>
<% }); -%>
<% } %>

<% if (doc.deprecated) { %>
<fieldset class="<%-styles['deprecated']-%>">
  <legend>Deprecated API</legend>
  <%- doc.deprecated -%>
</fieldset>
<% } %>

<div>
  <% if (doc.requires) { %>
  <h3>Dependencies</h3>
  <ul>
    <% doc.requires.forEach((require) => { %>
      <li>Add link here</li>
    <% }); %>
  </ul>
  <% } %>
</div>